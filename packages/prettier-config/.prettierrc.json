{
    "$schema": "http://json.schemastore.org/prettierrc",
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "overrides": [{
        "files": ["**/*.yml", "**/*.yaml"],
        "options": {
            "tabWidth": 2
        }
    }]
}
