{
    "$schema": "http://json.schemastore.org/prettierrc",
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "overrides": [
        {
            "files": ["**/*.yml", "**/*.yaml"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
